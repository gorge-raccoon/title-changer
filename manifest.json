{
  "manifest_version": 2,
  "name": "Title Changer",
  "version": "1.0",
  "description": "Modify web page titles based on a set of rules.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["title-changer.js"]
    }
  ],
  "permissions": ["storage"],
  "options_ui": {
    "page": "options.html"
  }
}

